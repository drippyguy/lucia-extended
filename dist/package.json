{
  "name": "lucia",
  "version": "3.0.1",
  "description": "A simple and flexible authentication library",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "files": [
    "/dist/",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "scripts/build.ts",
    "auri.build": "pnpm build",
    "test": "vitest run",
    "test.debug": "vitest run src/utils/debug.test.ts"
  },
  "keywords": [
    "lucia",
    "lucia-auth",
    "authentication",
    "auth"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pilcrowOnPaper/lucia",
    "directory": "packages/lucia"
  },
  "author": "pilcrowonpaper",
  "license": "MIT",
  "devDependencies": {
    "@babel/parser": "^7.24.0",
    "@types/node": "^18.6.2",
    "cpy": "^11.0.0",
    "prettier": "^2.3.0",
    "recast": "^0.23.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^0.33.0",
    "zx": "^7.2.3"
  },
  "dependencies": {
    "oslo": "github:drippyguy/oslo"
  },
  "exports": {
    "./core": {
      "import": {
        "types": "./core.d.ts",
        "default": "./core.js"
      },
      "require": {
        "types": "./core.d.cts",
        "default": "./core.cjs"
      },
      "types": "./core.d.ts",
      "default": "./core.js"
    },
    "./crypto.test": {
      "import": {
        "types": "./crypto.test.d.ts",
        "default": "./crypto.test.js"
      },
      "require": {
        "types": "./crypto.test.d.cts",
        "default": "./crypto.test.cjs"
      },
      "types": "./crypto.test.d.ts",
      "default": "./crypto.test.js"
    },
    "./crypto": {
      "import": {
        "types": "./crypto.d.ts",
        "default": "./crypto.js"
      },
      "require": {
        "types": "./crypto.d.cts",
        "default": "./crypto.cjs"
      },
      "types": "./crypto.d.ts",
      "default": "./crypto.js"
    },
    "./database": {
      "import": {
        "types": "./database.d.ts",
        "default": "./database.js"
      },
      "require": {
        "types": "./database.d.cts",
        "default": "./database.cjs"
      },
      "types": "./database.d.ts",
      "default": "./database.js"
    },
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "require": {
        "types": "./index.d.cts",
        "default": "./index.cjs"
      },
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./version": {
      "import": {
        "types": "./version.d.ts",
        "default": "./version.js"
      },
      "require": {
        "types": "./version.d.cts",
        "default": "./version.cjs"
      },
      "types": "./version.d.ts",
      "default": "./version.js"
    },
    "./scrypt/index.test": {
      "import": {
        "types": "./scrypt/index.test.d.ts",
        "default": "./scrypt/index.test.js"
      },
      "require": {
        "types": "./scrypt/index.test.d.cts",
        "default": "./scrypt/index.test.cjs"
      },
      "types": "./scrypt/index.test.d.ts",
      "default": "./scrypt/index.test.js"
    },
    "./scrypt": {
      "import": {
        "types": "./scrypt/index.d.ts",
        "default": "./scrypt/index.js"
      },
      "require": {
        "types": "./scrypt/index.d.cts",
        "default": "./scrypt/index.cjs"
      },
      "types": "./scrypt/index.d.ts",
      "default": "./scrypt/index.js"
    }
  }
}
